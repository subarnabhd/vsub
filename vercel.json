{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  {
    "routes": [
      { "src": "/css/(.*)", "dest": "/css/$1" }, // Route for CSS files
      { "src": "/js/(.*)", "dest": "/js/$1" },   // Route for JavaScript files
      { "src": "/images/(.*)", "dest": "/images/$1" }, // Route for image files
      { "src": "/(.*)", "dest": "/index.php" }  // Catch-all route for all other paths
    ]
  }
  
}
