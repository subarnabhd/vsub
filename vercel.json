{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    
    { "src": "/about", "dest": "/about.php" },
    { "src": "/tools", "dest": "/tools.php" },


    { "src": "/script/(.*)", "dest": "/script/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },



    { "src": "/image/(.*)", "dest": "/image/$1" },
    { "src": "/(.*)", "dest": "/index.php" }
  ]
}
