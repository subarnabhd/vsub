{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    


    { "src": "/script/(.*)", "dest": "/script/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },



    { "src": "/image/(.*)", "dest": "/image/$1" },
    { "src": "/(.*)",  "dest": "/api/index.php" }
  ]
}
